<div class="page-header">
  <%= link_to municipalities_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_municipality_path(@municipality), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show municipality</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @municipality.name %></dd>

  <dt>Region:</dt>
  <dd><%= link_to @municipality.region, @municipality.region %></dd>

</dl>

<h2>Locales of <%= @municipality.name %></h2>
<%= render "locales/table" %>
